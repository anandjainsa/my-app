node{
   stage('SCM Checkout'){
     git 'https://github.com/javahometech/my-app'
   }
   stage('Compile-Package'){
      // Get maven home path
      sh "/opt/maven3/bin/mvn package"
   }
   
   stage('SonarQube Analysis') {
        withSonarQubeEnv('sonar-6') { 
          sh "/opt/maven3/bin/mvn sonar:sonar"
        }
    }
   stage('Nexus Uploader') {
       nexusPublisher nexusInstanceId: 'mynexus', 
       nexusRepositoryId: 'nexus_repo', 
       packages: [[$class: 'MavenPackage', mavenAssetList: [], mavenCoordinate: [artifactId: 'myweb', groupId: 'in.javahome', packaging: 'war', version: '0.0.7']]]
   }
}
